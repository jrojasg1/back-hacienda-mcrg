{
  "name": "hacienda-mcrg",
  "version": "1.0.0",
  "description": "backend node + TS + express + swagger",
  "main": "index.js",
  "scripts": {
    "swagger": "tsoa spec",
    "dev": "concurrently \"npx tsc --watch\" \"npx run swagger\"  \"nodemon -q dist/index.js\"",
    "test": "jest",
    "serve:coverage": "cd coverage/lcov-report && npx serve",
    "build": "npx webpack --mode development",
    "start": "node dist/index.js",
    "build:prod": "npx webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrojasg1/back-hacienda-mcrg.git"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "swagger",
    "mongoose"
  ],
  "author": "@jrojasg1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrojasg1/back-hacienda-mcrg/issues"
  },
  "homepage": "https://github.com/jrojasg1/back-hacienda-mcrg#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.5.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
